using namespace std;
using ll = long long;

int main(){
    int n, m;
    cin >> n >> m;
    vector<int> path(n, 0);
    for(int i = 0; i < m; i++){
        int a, b;
        cin >> a >> b;
        path[a - 1]++;
        path[b - 1]++;
    }

    for(auto x : path){
        cout << x << endl;
    }
    return 0;
}
